{
    "name": "CSES searcher",
    "description": "fuck javascript and fuck google",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["storage","activeTab","scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/smiley16.png",
            "32": "/images/smiley32.png",
            "48": "/images/smiley48.png",
            "128": "/images/smiley128.png"
          }
      },
    "icons": {
        "16": "/images/smiley16.png",
        "32": "/images/smiley32.png",
        "48": "/images/smiley48.png",
        "128": "/images/smiley128.png"
    },
    
    "content_security_policy": {
        "extension_pages": "default-src 'self'; connect-src * 'unsafe-inline';"
    }    
  }
  